# Gemini API key (required)
# Used by the browser agent to authenticate with Google's Gemini API
# Get your key from: https://aistudio.google.com/app/apikey
export GEMINI_API_KEY="your-gemini-api-key-here"

# Metro.ca credentials for automated login (required)
# Used by the auth flow to automatically sign in to your Metro account
export GENERATIVE_SUPPLY_METRO_USERNAME="your-email@example.com"
export GENERATIVE_SUPPLY_METRO_PASSWORD="your-password-here"

# Camoufox browser profile directory (optional)
# Persistent storage for browser data (cookies, local storage, etc.)
# Defaults to: ~/.config/generative-supply/camoufox-profile
export GENERATIVE_SUPPLY_USER_DATA_DIR="~/.config/generative-supply/camoufox-profile"

